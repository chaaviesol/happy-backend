model staff_attendence {
  id             Int       @id @default(autoincrement())
  staff_id       Int?
  check_in       DateTime? @db.Timestamp(6)
  check_out      DateTime? @db.Timestamp(6)
  date           String?   @db.VarChar
  total_interval Float?
